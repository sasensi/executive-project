<?php

/**
 * @var \Zend\View\Renderer\PhpRenderer $this
 * @var array[]                         $createdProjectsData
 * @var array[]                         $transactionsData
 * @var array[]                         $transactionsAmountData
 */

$this->headScript()
     ->appendFile('https://code.highcharts.com/highcharts.js')
     ->appendFile('http://code.highcharts.com/maps/modules/map.js')
     ->appendFile('https://code.highcharts.com/mapdata/countries/fr/fr-all-all.js')
     ->appendScript('
        var createdProjectsData = '.json_encode($createdProjectsData).';
        var transactionsData = '.json_encode($transactionsData).';
        var transactionsAmountData = '.json_encode($transactionsAmountData).';
        ')
     ->appendFile($this->basePath('js/project/analyse.js'));

$this->headLink()->appendStylesheet($this->basePath('css/transaction/analyse.min.css'));

?>

<h1>analyse des projets</h1>

<div id="createdProjects"></div>

<div id="transactions"></div>

<div id="transactionsAmount"></div>